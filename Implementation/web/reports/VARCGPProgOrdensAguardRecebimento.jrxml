	<queryString>
		<![CDATA[SELECT 
	INAUFP.OrderNo,
	$$getUG^VARCompra(INAUFP.OrderNo)  as UG,
	$$SQLGetDescFornecedor^VARSQL(INAUF.SupplierNumber) as DescFornecedor,
	INAUF.Approval1 as Empenho,
	INAUF.FREE20 as IdPedidoCompra,
	$$getNoProcesso^VARPedidoCompra(INAUF.FREE20) as NoProcesso,
	INAUFP.Quantity as qtyLinha,
	(INAUFP.Quantity - $$getQuantidadeRecebida^VARCompra(INAUFP.OrderNo,INAUFP.LineItem)) as qtyPendente,
	$$getQuantidadeRecebida^VARCompra(INAUFP.OrderNo,INAUFP.LineItem) as qtyRecebidaLinha

FROM INAUF 
INNER JOIN INAUFP ON INAUF.OrderNumber = INAUFP.OrderNo

WHERE 
	INAUFP.ItemNumber = $P{idItem}
	AND INAUFP.FREE29 = $P{idProgram}
	AND INAUF.FREE14 >= 1 
	AND INAUF.FREE14 < 3
	AND (nvl(INAUFP.FREE10," ") = "" OR nvl(INAUFP.FREE10," ") = 0 OR nvl(INAUFP.FREE10," ") = 1 OR nvl(INAUFP.FREE10," ") = 2)]]>
	</queryString>
