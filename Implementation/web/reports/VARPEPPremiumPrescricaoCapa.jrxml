	<queryString>
		<![CDATA[select $$getTipoPrescricao^VARSQL(NumerodaPrescricao,NumerodaLinha) via,
paciente.name Nome,
(select top 1 NumerodaPrescricao from 
	VARPEPPremiumPrescricaoTratamento
