	<queryString>
		<![CDATA[SELECT 
count(rej.IDReject) as Quantidade, 
rej.RejectReason as Motivo
FROM MEDPrescription presc 
INNER JOIN MEDPatient pati ON pati.PatientID = presc.PatientID 
INNER JOIN MEDDispense disp ON disp.Prescription = presc.PrescriptionNumber 
INNER JOIN VARHistRejeicaoDisp rej ON $piece(rej.DispenseLineID ,"||",1) = disp.Dispensation
$P!{strWhere2}
$P!{strWhere}
GROUP BY rej.IDReject]]>
	</queryString>
