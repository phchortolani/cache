	<queryString>
		<![CDATA[SELECT TOP 5
	Location,
        $$SQLGetLocationName^VARSQL(Location) as DescLocation,
	$$GetEstoqueDisponivel^VARReposicao($P{idItem},Location) as Estoque,
	$$SQLGetStockOnHand^VARSQL($P{idItem},Location) as EstoqueTotal

FROM WWW0121
WHERE 
	$UPPER(StorageLocn) = 1
	AND $$GetEstoqueDisponivel^VARReposicao($P{idItem},Location) > 0
ORDER BY Estoque Desc]]>
