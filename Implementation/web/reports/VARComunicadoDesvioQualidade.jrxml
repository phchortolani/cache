	<queryString>
		<![CDATA[SELECT Ocorr.CodOcorrencia,
       TO_CHAR(Ocorr.Data, "DD/MM/YYYY") AS DataFmt,
       CASE NVL(Rec.InvoiceNumber,0)
       WHEN 0 THEN Ocorr.NotaFiscal
       ELSE Rec.InvoiceNumber END AS InvoiceNumber,
       CASE NVL(Rec.InvoiceDate,0)
       WHEN 0 THEN TO_CHAR(Ocorr.DataEmissaoNF, "DD/MM/YYYY")
       ELSE TO_CHAR(Rec.InvoiceDate, "DD/MM/YYYY") END AS InvoiceDateFmt,
       TO_CHAR(+Rec.ProcessedDate, "DD/MM/YYYY") AS ReceiptDateFmt,
       CASE NVL(+RecLine.UnitQuantity,0)
       WHEN 0 THEN Ocorr.QuantidadeRecebida
       ELSE +RecLine.UnitQuantity END AS UnitQuantity,
       CASE NVL(RecLine.Item,0)
       WHEN 0 THEN Ocorr.Produto || ' - ' || $$SQLGetDescricaoProduto^VARSQL(Ocorr.Produto) 
       ELSE RecLine.Item || ' - ' || $$SQLGetDescricaoProduto^VARSQL(RecLine.Item) END AS DescricaoProduto,
       +Ocorr.QuantidadeSegregada AS QuantidadeSegregada,
       Ocorr.Recorrencias,
       Para.Text AS RecorrenciasDesc,
       $$SQLGetMemoWithCRLF^VARTRKSQL(Ocorr.DescricaoDetalhada) AS DescricaoDetalhada,
       $$SQLGetMemoWithCRLF^VARTRKSQL(Ocorr.ParecerFornecedor) AS ParecerFornecedor,
       Ocorr.AcaoTomada,
       CASE NVL(RecLine.LotNumber,0)
       WHEN 0 THEN Ocorr.Lote
       ELSE RecLine.LotNumber END AS Lote,
       $$GetClassificacaoOcorrencia^VARSQL(Ocorr.Classificacao) as Classificacao,
       Ocorr.AcaoGTAMMCompras as AcaoGTAMMCompras,
       Ocorr.Responsavel as Responsavel,
       $$getNomeUsuario^VARSESJasperRunReport(Ocorr.Responsavel) as NomeResponsavel,

	FornecedorNotificado,
	NoNotificacao,
	DetalhesDaNotificacao,
	$$SQLGetFormatDate7^VARSQL(DatadaNotificacao) as DatadaNotificacao

 FROM SQLUser.VARPARA AS Para,
      SQLUser.VARFornecedorOcorrencia AS Ocorr
      
      LEFT JOIN SQLUser.INREC AS Rec ON
      Rec.Company = Ocorr.Company
      AND Rec.ReceiptNumber = Ocorr.Recebimento
 
      LEFT JOIN SQLUser.INRECLine AS RecLine ON
      RecLine.Company = Ocorr.Company
      AND RecLine.ReceiptNumber = Ocorr.Recebimento
      AND RecLine.LineNumber = Ocorr.LinhaRecebimento
      
 WHERE Ocorr.Company = 0
 AND Ocorr.CodFornecedor = $P{CodFornecedor}
 AND Ocorr.CodOcorrencia = $P{CodOcorrencia}
 AND Para.Company = 0
 AND Para.ParameterName = 'RECORRENCIADESVIOQUALIDADE'
 AND Para.Language1 = 'PT'
 AND Para.SearchWord = Ocorr.Recorrencias]]>
