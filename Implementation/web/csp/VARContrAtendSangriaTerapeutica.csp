					},
					load: function(args) {
						return $.getJSON(urlPreparacaoQui,{
							method:"getCID",
							usuario: YBED,
							skip:args.skip,
							take:args.take,
							searchValue:args.searchValue
						});
					}
				})
			})
		}).dxTagBox("instance");
