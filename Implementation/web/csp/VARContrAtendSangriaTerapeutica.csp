		campoDiagnostico = $("#textDiagnostico").dxTagBox({
			//height: 90,
			valueExpr: "Codigo", 
			displayExpr: "Descricao",
			searchEnabled: true,
			buttons: [{
				name: "btnPesquisarDiagnostico",
				location: "before",
				options: {
					hint: "Pesquisar",
