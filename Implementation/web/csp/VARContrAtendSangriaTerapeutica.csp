							}else{
								DevExpress.ui.notify("Erro: "+objeto.status,"error");
							}
					});
				}
				carregarTela = "";
			}
		}).dxSelectBox("instance");
		campoPaciente = $("#textPaciente").dxTextBox({readOnly: true, height: 27}).dxTextBox("instance");
