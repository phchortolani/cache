							}
						},{
							dataField: "software", colSpan: 2, editorType: "dxTagBox", label: {text:"Software:", alignment: "right"},
							editorOptions: { 
								dataSource: new DevExpress.data.DataSource({ 
									store: new DevExpress.data.CustomStore({
										loadMode: "raw", 
