</head>
<body>
	<div class="wrapper">
		<!-- Content Wrapper. Contains page content -->
		<div class='bg-white'>
			<!-- Content Header (Page header) -->
			<div class="content-header">
				<div class="container-fluid">
					<div class="row mb-2">
						<div class="col-sm-6">
							<h1 class="m-0 text-secondary">Paineis</h1>
						</div><!-- /.col -->
					</div><!-- /.row -->
				</div><!-- /.container-fluid -->
			</div>
			<!-- /.content-header -->
			<!-- Main content -->
			<section class="content">
				<div class="container-fluid">
					<!-- Small boxes (Stat box) -->
					<div class="row">
						<div class="col-lg-4 col-6">
							<!-- small box -->
							<div class="small-box bg-info">
								<div class="inner">
									<h5>&nbsp;</h5>
									<p>Posição de Estoque</p>
								</div>
								<div class="icon">
									<i class="ion ion-stats-bars"></i>
								</div>
								<a href="#" class="small-box-footer" onclick='abreBI(1)'>Visualizar <i class="fas fa-arrow-circle-right"></i></a>
							</div>
						</div>
						<!-- ./col -->
						<div class="col-lg-4 col-6">
							<!-- small box -->
							<div class="small-box bg-success">
								<div class="inner">
									<h5>&nbsp;</h5>
									<p>Procedimentos Realizados por Período</p>
								</div>
								<div class="icon">
									<i class="ion ion-stats-bars"></i>
								</div>
								<a href="#" class="small-box-footer" onclick='abreBI(2)'>Visualizar <i class="fas fa-arrow-circle-right"></i></a>
							</div>
						</div>
						<!-- ./col -->
						<div class="col-lg-4 col-6">
							<!-- small box -->
							<div class="small-box bg-warning">
								<div class="inner">
									<h5>&nbsp;</h5>
									<p>Tipo Consulta x Sexo</p>
								</div>
								<div class="icon">
									<i class="ion ion-stats-bars"></i>
								</div>
								<a href="#" target='myFrame' class="small-box-footer">Visualizar <i class="fas fa-arrow-circle-right"></i></a>
							</div>
						</div>
					</div>
					<!-- /.row -->
					<!-- Main row -->
					<div class="row">
						<!-- Left col -->
						<section class="col-12 connectedSortable">
							<div class="card bg-light">
								<div class="card-header">
									<h3 class="card-title">
										<i class="ion ion-clipboard mr-1"></i>Visão
									</h3>
								</div>
								<!-- /.card-header -->
								<div class="card-body m-0 p-0">
									<iframe name='myFrame' id='myFrame' height='350px' width='100%' frameborder=0 src=''></iframe>
								</div>
								<!-- /.card-body -->
							</div>
							<!-- /.card -->
						</section>
					<!-- right col -->
					</div>
				</div>
			</section>
		<!-- /.content -->
		</div>
	<!-- /.content-wrapper -->
	</div>
<!-- ./wrapper -->
