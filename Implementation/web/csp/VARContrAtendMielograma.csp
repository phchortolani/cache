							if (objeto.status==1){
								DevExpress.ui.notify("Leito alterado com Sucesso!","success");
							}else{
								DevExpress.ui.notify("Erro: "+objeto.status,"error");
							}
					});
				}
				carregarTela = "";
			}
		}).dxSelectBox("instance");
