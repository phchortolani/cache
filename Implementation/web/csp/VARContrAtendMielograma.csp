				$("#divCadastrarMedico").dxPopup("show");
			}
		}).dxButton("instance");	
		//
		campoMedico = $("#selectMedico").dxSelectBox({
			dataSource: dataSourceProfissional,valueExpr: "CodProvider", displayExpr: "DescProvider", height: 27}
