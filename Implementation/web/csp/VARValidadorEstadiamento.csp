			function prapararMAPPA(pSequencia) {
				$.get(urlPreparacao,{method:"PrapararMAPPA", pSequencia: pSequencia},function(rs){
					//$("#divLinhas").dxDataGrid("instance").option("dataSource").reload();
					dSLinha.reload()
				});
			}

			dSLinha = new DevExpress.data.DataSource({
	            store: new DevExpress.data.CustomStore({
					load: function() {
						var pCodCID = objForm.getEditor("cid").option("value");
						if (pCodCID == "" || pCodCID == null) return [];
						return $.getJSON(urlPreparacao,{method:"GetLinhas", pCodCID: pCodCID},function(rs){});
	        		},
	            })
    		});

			function montaDataGrid() {
				var pCodCID = objForm.getEditor("cid").option("value");
				if (pCodCID == "" || pCodCID == null) pCodCID = "";
			
				if ($("#divLinhasGrid").html()!="") $("#divLinhasGrid").remove();
			
				$("#divLinhas").html("<div id='divLinhasGrid'></div>");
				
				$("#divLinhasGrid").dxDataGrid({
					disabled: (pCodCID == "" ? true : false),
					dataSource: dSLinha,
					columns: (pCodCID == "" ? [] :eval(#server(VAR.CSP.VARValidadorEstadiamento.GetColunas(YBED,pCodCID))#)),
					columnChooser: { enabled: true, mode: "dragAndDrop"},
					//remoteOperations: true,
					allowColumnReordering: false,
					columnAutoWidth: false,
					showColumnLines: true,
					showRowLines: true,
					rowAlternationEnabled: true,
					hoverStateEnabled: true,
					showBorders: true,
					headerFilter: { visible: true },
					grouping: { autoExpandAll: false },
					groupPanel: { visible: false, allowColumnDragging: true },
					columnFixing: { enabled: false },
					filterRow: { visible: true },
					height: function() { return window.innerHeight - 130;},
					customizeColumns: function(columns) {
						$.each(columns, function (_, element) {
							var conteudo = element.caption;
							if (conteudo.split(" | ")[0] == "MDCALC") {
								element.headerCellTemplate = function(header, info) {
									var tipo =conteudo.split(" | ")[1];
									if (tipo == "Diagnóstico Mieloma Múltiplo") tipo = "Diagn";
									$('<div>').html(info.column.caption+"&nbsp; <span onClick=consultarReferencia('"+tipo+"'); title='Consultar site referência' style='color:green;' ><i fas class='fa fa-info-circle' style='cursor:pointer'></i></span>").appendTo(header);
								}
							}
						});
					},
					onToolbarPreparing: function (e) {
						var dataGrid = e.component;
						e.toolbarOptions.items.unshift({
							location: "after",
							widget: "dxButton",
							options: {
								icon: "fas fa-file-image",
								hint: "Conferir Regras",
								onClick: function (e) {
									$("#divValidacao").dxPopup("show");
								}
							}
						},{
							location: "after",
							widget: "dxButton",
							options: {
								icon: "fas fa-file-import",
								hint: "Importar Tabela",
								onClick: function (e) {
									var pCodCID = objForm.getEditor("cid").option("value");
									if (pCodCID == "" || pCodCID == null) {
										DevExpress.ui.notify("Selecione um CID.", "warning", 4000);
									}else if (pCodCID == "C90.0") {
										importarArquivo('008', "dSLinha.reload();");
									}else{
										importarArquivo('007', "dSLinha.reload();");
