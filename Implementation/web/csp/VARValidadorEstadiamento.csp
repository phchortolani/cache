			function colunaResultado(container, options) {
				var valor 	= options.value;
				var coluna 	= options.column.dataField;
				var cor = options.column.corColuna;
				piece = coluna.split("Resultado");
				if (piece[0] == "BINET"){
					var img = options.data.BINETResultadoImg+".png";	
				}else if (piece[0] == "RAI"){
					var img = options.data.RAIResultadoImg+".png";
				}else if (piece[0] == "CLLIPI"){
					var img = options.data.CLLIPIResultadoImg+".png";
				}else if (piece[0] == "ISS"){
					var img = options.data.ISSResultadoImg+".png";
				}else if (piece[0] == "RISS"){
					var img = options.data.RISSResultadoImg+".png";
				}else if (piece[0] == "Diag"){
					var img = options.data.DiagResultadoImg+".png";
				}
				var botao = "&nbsp; &nbsp;<i onClick=ExibirImagem('"+options.data.CIDImagem+"/"+piece[0]+"','"+img+"'); class='far fa-images' style='color:green; cursor: pointer;' aria-hidden='true'></i>";
				container.html("<span>"+valor+"</span>"+botao).css("background-color", cor);
			}
			
			function consultarReferencia(pTipo) {
				if (pTipo == "BINET"){
					window.open("https://www.mdcalc.com/binet-staging-system-chronic-lymphocytic-leukemia-cll");
				}else if (pTipo == "RAI"){
					window.open("https://www.mdcalc.com/rai-staging-system-chronic-lymphocytic-leukemia-cll");
				}else if (pTipo == "CLLIPI"){
					window.open("https://www.mdcalc.com/international-prognostic-index-chronic-lymphocytic-leukemia-cll-ipi#why-use");
				}else if (pTipo == "ISS"){
					window.open("https://www.mdcalc.com/multiple-myeloma-international-staging-system-iss");
				}else if (pTipo == "R-ISS"){
					window.open("https://www.mdcalc.com/revised-multiple-myeloma-international-staging-system-r-iss");
				}else if (pTipo == "Diagn"){
					window.open("https://www.mdcalc.com/multiple-myeloma-diagnostic-criteria");
				}
			}
			
			function mudarCor(container, options) {
				var valor 	= options.value;
				var cor = options.column.corColuna;
				container.html(valor).css("background-color", cor);
			}

			function colunaChec(container, options) {
				var valor = options.value;
				var cor = options.column.corColuna;
				if (valor == "Não OK") {
					var botao = "<i class='fas fa-thumbs-down' style='color:red;' aria-hidden='true'></i>";
				}else if (valor == "OK") {
					var botao = "<i class='fas fa-thumbs-up' style='color:blue;' aria-hidden='true'></i>";
				}else{
					var botao = "";
				}
				container.html(botao).css("background-color", cor);
			}
			
			function colunaConsultar(container, options) {
				var sequencia 	= options.data.Sequencia;
				var cCIDImagem 	= options.data.CIDImagem;
				var botao = "<i onClick=consultarMAPPA('"+sequencia+"','"+cCIDImagem+"'); class='fas fa-search-location' style='color:green;cursor:pointer;' aria-hidden='true'></i>";
				container.html(botao);
			}
			
			function consultarMAPPA(pSequencia, pCIDImagem) {
				$.getJSON(urlPreparacao,{method:"GetMAPPA", pSequencia: pSequencia, pCIDImagem: pCIDImagem},function(rs){
					if (pCIDImagem == "C90_0") {
						$("#divMAPPAC900").dxPopup("show");	
						$("#divMAPPAListC900").dxForm("instance").option("formData", rs)	
						//$("#divMAPPAList").dxForm("instance").itemOption("AVCSB.linfonodomegalia", "visible", 0);
						//if (rs.Campo69 == "S") {
						//	$("#divMAPPAList").dxForm("instance").itemOption("AVCSB.linfonodomegalia", "visible", 1);	
						//}else{
						//	$("#divMAPPAList").dxForm("instance").itemOption("AVCSB.linfonodomegalia", "visible", 0);
						//}
					}else{
						$("#divMAPPA").dxPopup("show");	
						$("#divMAPPAList").dxForm("instance").option("formData", rs)	
						$("#divMAPPAList").dxForm("instance").itemOption("AVCSB.linfonodomegalia", "visible", 0);
						if (rs.Campo69 == "S") {
							$("#divMAPPAList").dxForm("instance").itemOption("AVCSB.linfonodomegalia", "visible", 1);	
						}else{
							$("#divMAPPAList").dxForm("instance").itemOption("AVCSB.linfonodomegalia", "visible", 0);
						}
					}
				});
			}

			function preparar(container, options) {
				var sequencia = options.data.Sequencia;
				var botao = "<i onClick=prapararMAPPA('"+sequencia+"'); class='fas fa-search-location' style='color:red;' aria-hidden='true'></i>";
				container.html(botao);
			}
			

			function ExibirImagem(pPasta, pIMG) {
				$("#divIMG").dxPopup("show");	
				$("#divdivIMGExibir").html("<img src='#(YGIF)#ValidadorMAPPA/"+pPasta+"/"+pIMG+"'>")
			}


			
