<script language="Cache" runat="server">
	Set YBED = $get(%request.Data("YBED", 1))
	Set flagUsuarioMedico = +$$GetChecaMedico^VARSQL(YBED)
	SET URLPreparacao = "VAR.CSP.VARContrAtendTransfusional.cls"
</script>
<div class="container-fluid">
<div class="row">
